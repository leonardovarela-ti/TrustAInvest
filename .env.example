# E-Trade API credentials
# Sandbox credentials
ETRADE_SANDBOX_CONSUMER_KEY=your_sandbox_consumer_key
ETRADE_SANDBOX_CONSUMER_SECRET=your_sandbox_consumer_secret

# Production credentials
ETRADE_PROD_CONSUMER_KEY=your_production_consumer_key
ETRADE_PROD_CONSUMER_SECRET=your_production_consumer_secret

# Set to true for sandbox, false for production
ETRADE_SANDBOX=true

# Use these variables in docker-compose.yml
# For sandbox:
ETRADE_CONSUMER_KEY=${ETRADE_SANDBOX_CONSUMER_KEY}
ETRADE_CONSUMER_SECRET=${ETRADE_SANDBOX_CONSUMER_SECRET}

# For production (uncomment these and comment out the sandbox ones):
# ETRADE_CONSUMER_KEY=${ETRADE_PROD_CONSUMER_KEY}
# ETRADE_CONSUMER_SECRET=${ETRADE_PROD_CONSUMER_SECRET}

# Callback URL
ETRADE_CALLBACK_URL=oob

# Capital One API credentials
# Sandbox credentials
CAPITALONE_SANDBOX_CLIENT_ID=your_sandbox_client_id
CAPITALONE_SANDBOX_CLIENT_SECRET=your_sandbox_client_secret

# Production credentials
CAPITALONE_PROD_CLIENT_ID=your_production_client_id
CAPITALONE_PROD_CLIENT_SECRET=your_production_client_secret

# Set to true for sandbox, false for production
CAPITALONE_SANDBOX=true

# Use these variables in docker-compose.yml
# For sandbox:
CAPITALONE_CLIENT_ID=${CAPITALONE_SANDBOX_CLIENT_ID}
CAPITALONE_CLIENT_SECRET=${CAPITALONE_SANDBOX_CLIENT_SECRET}

# For production (uncomment these and comment out the sandbox ones):
# CAPITALONE_CLIENT_ID=${CAPITALONE_PROD_CLIENT_ID}
# CAPITALONE_CLIENT_SECRET=${CAPITALONE_PROD_CLIENT_SECRET}

# Redirect URI for OAuth callback
CAPITALONE_REDIRECT_URI=https://trustainvest.com/api/v1/capitalone/auth/callback
